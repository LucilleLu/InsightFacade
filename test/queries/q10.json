{
    "title": "find coverage of all cases that with * at the beginning",
    "query": {
        "WHERE": {
            "AND" : [
                {
                    "IS": {
                        "courses_dept":"*a"
                    }
                },
                {
                    "IS": {
                        "courses_id":"*8"
                    }
                },
                {
                    "IS": {
                        "courses_instructor":"*a"
                    }
                },
                {
                    "IS": {
                        "courses_title":"*a"
                    }
                },
                {
                    "IS": {
                        "courses_uuid":"*1"
                    }
                }
            ]
        },
        "OPTIONS": {
            "COLUMNS": ["courses_dept", "courses_avg"],
            "FORM": "TABLE"
        }
    }
    ,
    "response": {
        "code": 200,
        "body": {"result":[{"courses_dept":"asia","courses_avg":75.63}]
    }}
}
